#include <bits/stdc++.h>
using namespace std;

//CODE FOR REVERSE THE ARRAY
int main()
{
   int size;
   cin>>size;
   int a[size];
    for (int i = 0; i <size ; ++i) {
        cin>>a[i];
    }
    sort(a,a+size);
    cout<<"MAXIMUM ELEMENT IS :"<<a[size-1]<<endl;
    cout<<"MINIMUM ELEMENT IS :"<<a[0]<<endl;
    return 0;
}



#include <bits/stdc++.h>
using namespace std;

//CODE FOR REVERSE THE ARRAY WITHOUT INBUILT FUNCTION
int main()
{
   int size;
   cin>>size;
   int a[size],max=0,min;
    for (int i = 0; i <size ; ++i) {
        cin>>a[i];
    }
    min=a[0];
    for (int i = 0; i <size ; ++i) {
        if (a[i]>max)
        {
            max=a[i];
        } else if (a[i]<=min)
        {
            min=a[i];
        }
    }
    cout<<"MAXIMUM ELEMENT IS :"<<max<<endl;
    cout<<"MINIMUM ELEMENT IS :"<<min<<endl;
    return 0;
}
